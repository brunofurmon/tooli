{
  "name": "@tooli/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx dev tooli",
    "start": "nx start tooli",
    "build": "nx run-many --target=build --all",
    "build:all": "nx run-many --target=build --all",
    "build:affected": "nx affected --target=build",
    "test": "nx run-many --target=test --all",
    "test:affected": "nx affected --target=test --parallel=1 -- --runInBand --forceExit --passWithNoTests",
    "test:watch": "nx test --watch",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "lint:affected": "nx affected --target=lint",
    "e2e": "nx e2e tooli-e2e",
    "e2e:affected": "nx affected:e2e",
    "typecheck": "nx run-many --target=typecheck --all",
    "clean": "nx reset",
    "graph": "nx graph",
    "affected:graph": "nx affected:graph",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "wheel:dev": "nx dev tooli --port=3000",
    "wheel:start": "nx start tooli --port=3000",
    "wheel:build": "nx build tooli --skip-nx-cache",
    "wheel:test": "nx test tooli",
    "wheel:lint": "nx lint tooli",
    "wheel:e2e": "nx e2e tooli-e2e",
    "libs:build": "nx run-many --target=build --projects=wheel-engine,user-management,history-tracker,shared-ui,audio-system",
    "libs:test": "nx run-many --target=test --projects=wheel-engine,user-management,history-tracker,shared-ui,audio-system",
    "libs:lint": "nx run-many --target=lint --projects=wheel-engine,user-management,history-tracker,shared-ui,audio-system",
    "precommit": "npm run lint && npm run test:affected && npm run build:all",
    "prepare": "husky install",
    "docs:dev": "nx dev tooli --port=3001",
    "docs:build": "nx build tooli",
    "docs:serve": "nx serve-static tooli --port=3001",
    "health:check": "node scripts/health-check.js",
    "health:monitor": "node scripts/route-monitor.js",
    "dev:monitored": "npm run health:monitor & npm run wheel:dev",
    "test:health": "nx test tooli --testPathPatterns=health.spec.ts",
    "test:routing": "nx test tooli --testPathPatterns=routing.spec.ts",
    "test:all": "npm run test && npm run test:health && npm run test:routing",
    "monitor:routes": "node scripts/route-monitor.js",
    "monitor:routes:fast": "node scripts/route-monitor.js --fast",
    "dev:auto-monitor": "npm run monitor:routes & npm run wheel:dev",
    "dev:auto-monitor:fast": "npm run monitor:routes:fast & npm run wheel:dev",
    "check:alignment": "node scripts/visual-alignment-check.js",
    "check:all": "npm run health:check && npm run check:alignment",
    "dev:full-monitor": "npm run check:all & npm run dev:auto-monitor"
  },
  "private": true,
  "dependencies": {
    "@heroui/react": "^2.8.2",
    "framer-motion": "^12.23.12",
    "i18next": "^25.3.6",
    "next": "~15.2.4",
    "next-i18next": "^15.4.2",
    "next-themes": "^0.4.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.6.1",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "^9.8.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@nx/devkit": "21.4.0",
    "@nx/eslint": "21.4.0",
    "@nx/eslint-plugin": "21.4.0",
    "@nx/jest": "21.4.0",
    "@nx/js": "21.4.0",
    "@nx/next": "21.4.0",
    "@nx/playwright": "21.4.0",
    "@nx/react": "21.4.0",
    "@nx/vite": "21.4.0",
    "@nx/web": "21.4.0",
    "@nx/workspace": "21.4.0",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.38",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.0.0",
    "ajv": "^8.0.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.2",
    "eslint": "^9.8.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "^8.0.3",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^15.2.0",
    "nx": "21.4.0",
    "postcss": "^8.5.6",
    "prettier": "^2.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^3.0.0"
  },
  "workspaces": [
    "apps/*",
    "libs",
    "libs/*"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
